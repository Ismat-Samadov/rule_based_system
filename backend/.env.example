# Backend Environment Variables
# Copy this file to .env and update values
#
# IMPORTANT: The backend now loads .env from the PROJECT ROOT directory
# This file is kept for reference, but you should configure the root .env file

# Google Gemini API Key (REQUIRED for AI chatbot)
# Get your free API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Debug mode (set to False in production)
DEBUG=False

# CORS Origins - Add your frontend URLs (comma-separated)
# Example: https://your-frontend.onrender.com,https://your-app.com
CORS_ORIGINS=https://your-frontend-url.onrender.com
